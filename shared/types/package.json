{
  "name": "@authscript/types",
  "version": "0.1.0",
  "description": "Shared TypeScript types for AuthScript",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./common": {
      "types": "./dist/common.d.ts",
      "default": "./dist/common.js"
    },
    "./authscript": {
      "types": "./dist/authscript.d.ts",
      "default": "./dist/authscript.js"
    },
    "./cds": {
      "types": "./dist/cds.d.ts",
      "default": "./dist/cds.js"
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "test": "vitest run"
  },
  "devDependencies": {
    "typescript": "^5.3.0",
    "vitest": "^3.2.2"
  }
}
